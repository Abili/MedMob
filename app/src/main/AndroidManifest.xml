<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raisac.medmob">

    <application
        android:allowBackup="true"
        android:icon="@drawable/medmob_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/medmob_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DoctorInfoPage"></activity>
        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".OptionsPage" />
        <activity android:name=".LookForDoctor" />
        <activity android:name=".AwaitingPage" />
        <activity android:name=".UserProfile">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>