<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raisac.medmob">

    <application
        android:allowBackup="true"
        android:icon="@drawable/medmod_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/medmod_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OptionsPage"></activity>
        <activity android:name=".LookForDoctor" />
        <activity android:name=".SignUpDoctors" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>